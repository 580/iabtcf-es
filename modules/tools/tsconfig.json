{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "importHelpers": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "types": [
      "webpack-env"
    ],
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "tests/node_modules/@iabtcf/core/lib/*.*"
  ],
  "exclude": [
    "node_modules"
  ]
}

{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "rootDir": "src",
    "outDir": "lib"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*"
  ],
  "typedocOptions": {
    "theme": "markdown",
    "mode": "file",
    "readme": "none",
    "name": "@iabtcf/core - API Documentation",
    "out": "docs/api",
    "excludePrivate":true
  }
}


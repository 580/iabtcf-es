{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "declaration": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": false
  }
}
